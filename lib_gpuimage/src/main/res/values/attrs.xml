<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="gpuimage_surface_type" format="enum">
        <enum name="surface_view" value="0" />
        <enum name="texture_view" value="1" />
    </attr>
    <attr name="gpuimage_show_loading" format="boolean" />

    <declare-styleable name="GPUImageView">
        <attr name="gpuimage_surface_type" />
        <attr name="gpuimage_show_loading" />
    </declare-styleable>

    <declare-styleable name="JdGPUDisplayView">
        <attr name="gpuimage_surface_type" />
        <attr name="gpuimage_show_loading" />
        <attr name="display_media" format="enum">
            <enum name="render_img" value="0" />
            <enum name="render_video" value="1" />
        </attr>
    </declare-styleable>

</resources>